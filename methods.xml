<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="5">
	<test thread-count="5" name="Test">
		<classes>
			<class name="allLeads.DeleteLead">
				<methods>
					<include name="deleteLead"></include>
					<exclude name="sampleMethod"></exclude>
				</methods>
			</class>
			<class name="allLeads.DuplicateLead">
				<methods>
					<include name="duplicateLead"></include>
				</methods>
			</class>
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->



<!-- each method will be considered as each thread/testcases-->